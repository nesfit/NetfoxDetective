<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net"
      type="log4net.Config.Log4NetConfigurationSectionHandler, 
            log4net"/>
  </configSections>
  <log4net>
    <appender name="NullAppender" type="Netfox.Logger.NullAppender,  Logger" />
    <root>
      <level value="Error" />
      <appender-ref ref="NullAppender" />
    </root>
  </log4net>
</configuration>

