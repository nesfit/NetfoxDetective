    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Castle.Windsor" Version="5.1.1" />
      <PackageReference Include="CefSharp.Common" Version="89.0.170" />
      <PackageReference Include="CefSharp.Wpf" Version="89.0.170" />
    </ItemGroup>

</Project>
