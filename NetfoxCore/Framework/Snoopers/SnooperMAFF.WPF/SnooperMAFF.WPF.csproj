        <RootNamespace>Netfox.Snoopers.SnooperMAFF.WPF</RootNamespace>
        <CefSharpAnyCpuSupport>true</CefSharpAnyCpuSupport>
        <PlatformTarget>x64</PlatformTarget>
        <UseWpf>true</UseWpf>
        <OutDir>$(SolutionDir)\Detective\App\Netfox.Detective\bin\$(Configuration)\$(TargetFramework)</OutDir>
