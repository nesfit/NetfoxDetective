    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Castle.Windsor" Version="5.1.1" />
      <PackageReference Include="Telerik.Windows.Controls.Charting.for.Wpf.Xaml.Trial" Version="2021.1.325" />
